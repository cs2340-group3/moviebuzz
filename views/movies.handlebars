<ul class="movie_list">
  {{#each movies}}
  <div class="row">
  <div class="col-md-4 col-md-4">
    <div class="thumbnail">
      <img src="{{this.posters.original}}" alt="Profile" height ="50" width="200">
      <div class="caption">
        <h3>{{this.title}} ({{this.year}})</h3>
        <p>description</p>
      </div>
    </div>
  </div>
</div>
  {{else}}
    <p>No result</p>
  {{/each}}
</ul>
