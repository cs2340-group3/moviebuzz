<ul class="movie_list">
  {{#each movies}}
  

<div class="media">
  <div class="media-left">
    <a href="#">
      <img class="media-object" src="{{this.posters.detailed}}" alt="moviePoster">
    </a>
  </div>
  <div class="media-body">
    <li><a href="/movie/{{this.id}}">{{this.title}}</a> ({{this.year}})</li>
    
   <p>Description: {{this.synopsis}}</p>
  
   
    <p>Rated : {{this.mpaa_rating}} </p>
  </div>
</div>
        
    
  {{else}}
    <p>No result</p>
  {{/each}}
  
</ul>
